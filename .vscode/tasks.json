{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "make <driver packs>",
            "type": "shell",
            "group": "build",
            "command": "make",
            "args": [
                "${input:pickDriverPacks}",
            ],
            "problemMatcher":{
                "pattern":{
                    "regexp": "^(make:)\\s\\*\\*\\*(.*)$",
                    "kind": "file",
                    "message": 2 
                }
            }
        },
        {
            "label": "make <jetpack options>",
            "type": "shell",
            "group": "build",
            "command": "make",
            "args": [
                "${input:pickJetPacks}"
            ],
            "problemMatcher":{
                "pattern":{
                    "regexp": "^(make:)\\s\\*\\*\\*(.*)$",
                    "kind": "file",
                    "message": 2 
                }
            }
        },
        {
            "label": "make <other options>",
            "type": "shell",
            "group": "build",
            "command": "make",
            "args": [
                "${input:pickOthers}"
            ],
            "problemMatcher":{
                "pattern":{
                    "regexp": "^(make:)\\s\\*\\*\\*(.*)$",
                    "kind": "file",
                    "message": 2 
                }
            }
        },
        {
            "label": "make <jetpack dep options>",
            "type": "shell",
            "group": "build",
            "command": "make",
            "args": [
                "${input:pickDependencyBuilds}"
            ],
            "problemMatcher":{
                "pattern":{
                    "regexp": "^(make:)\\s\\*\\*\\*(.*)$",
                    "kind": "file",
                    "message": 2 
                }
            }
        },
        {
            "label": "make <imaging options>",
            "type": "shell",
            "group": "build",
            "command": "make",
            "args": [
                "image-l4t-${input:pickImagingBuilds}"
            ],
            "problemMatcher":{
                "pattern":{
                    "regexp": "^(make:)\\s\\*\\*\\*(.*)$",
                    "kind": "file",
                    "message": 2 
                }
            }
        },
        {
            "label": "make <CTI imaging options>",
            "type": "shell",
            "group": "build",
            "command": "make",
            "args": [
                "image-${input:pickCtiImagingBuilds}"
            ],
            "problemMatcher":{
                "pattern":{
